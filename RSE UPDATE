function AKFirerate()
    local aux = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Upbolt/Hydroxide/revision/ohaux.lua"))()

    local scriptPath = game:GetService("Players").LocalPlayer.Backpack.AK.GunClient
    local closureName = "OnSwitching"
    local upvalueIndex = 2
    local closureConstants = {
        [1] = "Reloading",
        [2] = "Overheated",
        [3] = "ActuallyEquipped",
        [4] = "Enabled",
        [5] = "Inspecting",
        [6] = "Switching"
    }

    local closure = aux.searchClosure(scriptPath, closureName, upvalueIndex, closureConstants)
    local value = 0.01
    local elementIndex = "FireRate"

    debug.getupvalue(closure, upvalueIndex)[elementIndex] = value
end

--MISC

function InjectVirus()
    for i,v in pairs(game.Workspace.Rooms:GetDescendants()) do
        if v.Parent.Name == "Human" then
            if tostring(v.Parent.Parent.Owner.Value) == game.Players.LocalPlayer.Name then
                local Human = v
                local TPback = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                wait(0.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-21.5400715, 4.00000238, 206.451218, -0.199107915, 8.71226258e-10, -0.979977548, 5.94349148e-09, 1, -3.18548077e-10, 0.979977548, -5.88791371e-09, -0.199107915)
                wait(0.3)
                fireproximityprompt(game:GetService("Workspace").Viruses.random.rand.ProximityPrompt, 1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Human.HumanoidRootPart.CFrame * CFrame.new(0,0,3.5)
                wait(0.2)

                if game.Players.LocalPlayer.Backpack:FindFirstChild("Needlegun") then
                    game.Players.LocalPlayer.Backpack.Needlegun.Parent = game.Players.LocalPlayer.Character
                    local tool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
                    tool:Activate()
                    wait(0.2)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = TPBack
                else
                    local tool = game.Players.LocalPlayer.HumanoidRootPart.Character:FindFirstChildOfClass("Tool")
                    tool:Activate()
                    wait(0.2)
                    game.Players.LocalPlayer.Character.CFrame = TPBack
                end
            end
        end
    end
end
function GetCoffee()
    local TPBack = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-88.9132233, 4.00000334, 196.408691, 0.998212218, -1.82899136e-08, -0.0597692318, 1.25383002e-08, 1, -9.66053548e-08, 0.0597692318, 9.56832409e-08, 0.998212218)
    game.Players.LocalPlayer.Backpack.Mug.Parent = game.Players.LocalPlayer.Character
    fireclickdetector(game:GetService("Workspace").CoffeMachine.Button2.ClickDetector)
    wait(5)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = TPBack
end
function AnnoyTheServer()
    while wait(0.01) do
        local ohString1 = "PlayAudio"
        local ohTable2 = {
            ["Echo"] = true,
            ["DistantSoundVolume"] = 1.5,
            ["Origin"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
            ["Instance"] = game:GetService("Players").LocalPlayer.Backpack.USP.Handle["1"].FireSounds.FireSound,
            ["DistantSoundIds"] = {
                [1] = 177174605
            },
            ["Silenced"] = false
        }
        local ohTable3 = {
            ["AmmoPerMag"] = 12,
            ["RaisePitch"] = true,
            ["Origin"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
            ["Instance"] = game:GetService("Players").LocalPlayer.Backpack.USP.Handle["1"].LowAmmoSound,
            ["CurrentAmmo"] = 100000000000000000000
        }
        local ohBoolean4 = true

        game:GetService("ReplicatedStorage").Events.gunEvent:Fire(ohString1, ohTable2, ohTable3, ohBoolean4)
    end
end
function Respawn()
    game:GetService("ReplicatedStorage").sh2:FireServer()
end
function FireTouchInterests()
    for i,v in pairs(game.Workspace:GetDescendants()) do
        if v:IsA("TouchTransmitter") then
            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
            wait()
            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)
        end
    end
end
function FireProximityPrompts()
    for i,v in pairs(game.Workspace:GetDescendants()) do
        if v.Name == "ProximityPrompt" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.CFrame
            wait(0.2)
            fireproximityprompt(v, 1)
        end
    end
end
function FireClickDetectors()
    for i,v in pairs(game.Workspace:GetDescendants()) do
        if v:IsA("ClickDetector") then
            fireclickdetector(v)
        end
    end
end

