    wait(0.5)
    fireproximityprompt(game:GetService("Workspace").IgnoreParts.Characters.TaskUniforms.Part.ProximityPrompt, 1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = TPBack
